  import: stuff
  hs-source-dirs: exe
  main-is: Main.hs
  build-depends:
    base -any,
    plutus-metatheory
    
test-suite test1
  import: stuff
  build-tool-depends: plutus-core:plc
  hs-source-dirs: test
  build-depends:
    base -any,
    plutus-metatheory,
    process -any
  type:  exitcode-stdio-1.0
  main-is: TestSimple.hs

test-suite test2
  import: stuff
  build-tool-depends: plutus-core:plc
  hs-source-dirs: test
  type: detailed-0.9
  test-module: TestDetailed
  build-depends:
    base -any,
    bytestring -any,
    Cabal -any,
    directory -any,
    plutus-metatheory,
    process -any,
    text -any
    
test-suite test3
  import: stuff
  hs-source-dirs: test
  type: exitcode-stdio-1.0
  main-is: TestNEAT.hs
  build-depends:
    base -any,
    lazy-search -any,
    mtl -any,
    plutus-metatheory,
    plutus-core,
    size-based -any,
    Stream -any,
    tasty -any,
    tasty-hunit -any