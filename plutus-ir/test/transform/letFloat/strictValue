(lam x (con integer)
(let (nonrec)
     (termbind (nonstrict) (vardecl i (con integer))
      (let (nonrec)
           (termbind (strict) (vardecl j (con integer))
            (con 1))
            [(builtin addInteger) j x])
       )
     i
 ))
-- j should float before i, because i depends on j and j is floatable (e.g. strictValue)

