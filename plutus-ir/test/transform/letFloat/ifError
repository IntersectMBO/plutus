[ (lam b (all a (type) (fun (fun (con integer) a) (fun (fun (con integer) a) a)))
    [ b
	  (lam z (con integer)
        (let
          (nonrec)
          (termbind (strict) (vardecl err (con integer)) (error (con integer)))
      	  err
        )
      )
      (lam z (con integer) (con 1))
	])
  (abs a (type) (lam x (fun (con integer) a) (lam y (fun (con integer) a) [y (con 0)])))
]
