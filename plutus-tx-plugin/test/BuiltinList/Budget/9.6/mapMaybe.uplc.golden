(program
   1.1.0
   (\xs ->
      (\s -> s s)
        (\s xs ->
           force
             (force (force chooseList)
                xs
                (delay [])
                (delay
                   ((\x xs ->
                       force
                         (case
                            (force
                               (force ifThenElse
                                  (equalsInteger 0 (modInteger x 2))
                                  (delay (constr 1 []))
                                  (delay (constr 0 [x]))))
                            [ (\y -> delay (force mkCons y (s s xs)))
                            , (delay (s s xs)) ]))
                      (force headList xs)
                      (force tailList xs)))))
        xs))