program
  1.1.0
  (let
    data Bool | Bool_match where
      True : Bool
      False : Bool
    !ifThenElse : all a. bool -> a -> a -> a = ifThenElse
    !verifyEd25519Signature : bytestring -> bytestring -> bytestring -> bool
      = verifyEd25519Signature
    ~verifyEd25519Signature : bytestring -> bytestring -> bytestring -> Bool
      = \(pubKey : bytestring) ->
          let
            !pubKey : bytestring = pubKey
          in
          \(message : bytestring) ->
            let
              !message : bytestring = message
            in
            \(signature : bytestring) ->
              let
                !signature : bytestring = signature
                !b : bool = verifyEd25519Signature pubKey message signature
              in
              ifThenElse {Bool} b True False
  in
  \(ds : bytestring) ->
    let
      !ds : bytestring = ds
    in
    \(ds : bytestring) ->
      let
        !ds : bytestring = ds
      in
      \(ds : bytestring) ->
        let
          !ds : bytestring = ds
        in
        verifyEd25519Signature ds ds ds)