let
  data (Example :: * -> *) a | Example_match where
    EInt : integer -> Example a
    ETwo : a -> a -> Example a
  ~`$WETwo` : all a. a -> a -> Example a
    = /\a ->
        \(dt : a) (dt : a) ->
          let
            !dt : a = dt
            !dt : a = dt
          in
          ETwo {a} dt dt
  ~`$bERec` : all a. a -> a -> Example a
    = /\a ->
        \(hello : a) (world : a) ->
          let
            !hello : a = hello
            !world : a = world
          in
          `$WETwo` {a} hello world
  ~`$mERec` : all r a. Example a -> (a -> a -> r) -> (unit -> r) -> r
    = /\r a ->
        \(scrut : Example a) (cont : a -> a -> r) (fail : unit -> r) ->
          let
            !scrut : Example a = scrut
            !cont : a -> a -> r = cont
            !fail : unit -> r = fail
          in
          Example_match
            {a}
            scrut
            {r}
            (\(ipv : integer) -> fail ())
            (\(hello : a) (world : a) -> cont hello world)
  !r : Example string = `$bERec` {string} "wot" "yo"
in
`$mERec`
  {string}
  {string}
  r
  (\(ds : string) (ds : string) -> ds)
  (\(void : unit) -> "no")