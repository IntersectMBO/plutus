program
  1.1.0
  (let
    data Bool | Bool_match where
      True : Bool
      False : Bool
    ~andExternal : Bool -> Bool -> Bool
      = \(a : Bool) ->
          let
            !a : Bool = a
          in
          \(b : Bool) ->
            let
              !b : Bool = b
            in
            Bool_match
              a
              {all dead. Bool}
              (/\dead -> b)
              (/\dead -> False)
              {all dead. dead}
  in
  andExternal True False)