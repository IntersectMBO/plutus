letrec
  !go : list data -> bool
    = \(xs : list data) ->
        case
          bool
          xs
          [ False
          , (\(h : data) (t : list data) ->
               case
                 (all dead. bool)
                 (equalsData (I 8) h)
                 [(/\dead -> go t), (/\dead -> True)]
                 {all dead. dead}) ]
in
\(eta : (\a -> list data) integer) -> go eta