let
  data Bool | Bool_match where
    True : Bool
    False : Bool
in
letrec
  data (List :: * -> *) a | List_match where
    Nil : List a
    Cons : a -> List a -> List a
in
letrec
  !and : List Bool -> Bool
    = \(ds : List Bool) ->
        List_match
          {Bool}
          ds
          {all dead. Bool}
          (/\dead -> True)
          (\(x : Bool) (xs : List Bool) ->
             /\dead ->
               Bool_match
                 x
                 {all dead. Bool}
                 (/\dead -> and xs)
                 (/\dead -> False)
                 {all dead. dead})
          {all dead. dead}
in
and
  ((let
       a = List Bool
     in
     \(c : Bool -> a -> a) (n : a) ->
       c
         True
         (c
            True
            (c
               True
               (c
                  True
                  (c True (c True (c True (c True (c True (c True n))))))))))
     (\(ds : Bool) (ds : List Bool) -> Cons {Bool} ds ds)
     (Nil {Bool}))