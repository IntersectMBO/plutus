program
  1.1.0
  ((\f -> (\s -> s s) (\s x -> f (s s) x))
     (\go ds ->
        force
          (case
             ds
             [ (delay (constr 0 []))
             , (\x xs ->
                  delay
                    (force
                       (force ifThenElse
                          (lessThanEqualsInteger 1 x)
                          (delay (constr 1 []))
                          (delay (go xs))))) ]))
     (constr 0 []))