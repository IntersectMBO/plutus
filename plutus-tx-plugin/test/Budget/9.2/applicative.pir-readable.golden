            {all dead. Maybe b}
            (\(a : integer) -> /\dead -> Just {b} (f a))
            (/\dead -> Nothing {b})
            {all dead. dead})
        (\(x : integer) (y : integer) -> addInteger x y)
        x
in
Maybe_match
  {integer -> integer}
  ds
  {all dead. Maybe integer}
  (\(ipv : integer -> integer) ->
     /\dead ->
       Maybe_match
