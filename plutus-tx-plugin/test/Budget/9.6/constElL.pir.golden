letrec
  data (List :: * -> *) a | List_match where
    Nil : List a
    Cons : a -> List a -> List a
in
letrec
  !go : integer -> List integer -> integer
    = \(acc : integer) (ds : List integer) ->
        List_match
          {integer}
          ds
          {all dead. integer}
          (/\dead -> acc)
          (\(x : integer) (xs : List integer) -> /\dead -> go x xs)
          {all dead. dead}
in
let
  data Bool | Bool_match where
    True : Bool
    False : Bool
in
letrec
  !go : integer -> List integer
    = \(n : integer) ->
        Bool_match
          (ifThenElse {Bool} (lessThanEqualsInteger n 0) True False)
          {all dead. List integer}
          (/\dead -> Nil {integer})
          (/\dead -> Cons {integer} 1 (go (subtractInteger n 1)))
          {all dead. dead}
in
let
  !ls : List integer = go 1000
in
go 42 ls