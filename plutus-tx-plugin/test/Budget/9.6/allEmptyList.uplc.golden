(program
   1.1.0
   ((\s -> s s)
      (\s ds ->
         case
           ds
           [ (constr 0 [])
           , (\x xs ->
                force
                  (force ifThenElse
                     (lessThanEqualsInteger 1 x)
                     (delay (constr 1 []))
                     (delay (s s xs)))) ])
      (constr 0 [])))