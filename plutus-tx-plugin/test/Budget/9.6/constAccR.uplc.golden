(program
   1.1.0
   ((\go ->
       (\s -> s s)
         (\s ds -> force (case ds [(delay 42), (\x xs -> delay (s s xs))]))
         (go 1000))
      ((\s -> s s)
         (\s n ->
            force
              (force ifThenElse
                 (lessThanEqualsInteger n 0)
                 (delay (constr 0 []))
                 (delay
                    (constr 1 [1, ((\x -> s s x) (subtractInteger n 1))])))))))