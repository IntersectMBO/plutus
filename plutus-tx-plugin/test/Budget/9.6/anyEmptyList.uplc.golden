program
  1.1.0
  ((\s -> s s)
     (\s ds ->
        force
          (case
             ds
             [ (delay (constr 1 []))
             , (\x xs ->
                  delay
                    (force
                       (force ifThenElse
                          (lessThanEqualsInteger 1 x)
                          (delay (s s xs))
                          (delay (constr 0 []))))) ]))
     (constr 0 []))