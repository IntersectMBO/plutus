(program
   1.1.0
   ((\x0 ->
       (\s -> s s)
         (\s ds ->
            force
              (case
                 ds
                 [ (delay (constr 0 []))
                 , (\x xs ->
                      delay
                        (force
                           (force
                              (force ifThenElse
                                 (lessThanInteger x 0)
                                 (delay (delay (constr 1 [])))
                                 (delay (delay (s s xs))))))) ]))
         x0)
      ((\s -> s s)
         (\s n ->
            force
              (force
                 (force ifThenElse
                    (lessThanEqualsInteger n 0)
                    (delay (delay (constr 0 [])))
                    (delay
                       (delay
                          (constr 1
                             [0, ((\x -> s s x) (subtractInteger n 1))]))))))
         1000)))