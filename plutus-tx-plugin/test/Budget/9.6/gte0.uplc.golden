(program
   1.1.0
   ((\s -> s s)
      (\s ds ->
         force
           (case
              ds
              [ (delay (constr 0 []))
              , (\x xs ->
                   delay
                     (force
                        (case
                           (lessThanInteger x 0)
                           [(delay (s s xs)), (delay (constr 1 []))]))) ]))
      ((\s -> s s)
         (\s n ->
            force
              (case
                 (lessThanEqualsInteger n 0)
                 [ (delay (constr 1 [0, ((\x -> s s x) (subtractInteger n 1))]))
                 , (delay (constr 0 [])) ]))
         1000)))