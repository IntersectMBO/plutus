(program
   1.1.0
   ((\go ->
       (\s -> s s)
         (\s ds ->
            force
              (case
                 ds
                 [ (delay (constr 0 []))
                 , (\x xs ->
                      delay
                        (force
                           (force ifThenElse
                              (lessThanInteger x 0)
                              (delay (constr 1 []))
                              (delay (s s xs))))) ]))
         (go 1000))
      ((\s -> s s)
         (\s n ->
            force
              (force ifThenElse
                 (lessThanEqualsInteger n 0)
                 (delay (constr 0 []))
                 (delay
                    (constr 1 [0, ((\x -> s s x) (subtractInteger n 1))])))))))