(program
   1.1.0
   ((\go ->
       (\ls -> go 0 ls)
         ((\s -> s s)
            (\s x lim ->
               force
                 (force ifThenElse
                    (lessThanEqualsInteger x lim)
                    (delay (constr 1 [x, ((\x -> s s x) (addInteger 1 x) lim)]))
                    (delay (constr 0 []))))
            1
            1000))
      ((\s -> s s)
         (\s acc ds ->
            case ds [acc, (\x xs -> (\x -> s s x) (addInteger acc x) xs)]))))