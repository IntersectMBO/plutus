(program
   1.1.0
   ((\go ->
       (\ls -> go 0 ls)
         ((\s -> s s)
            (\s x lim ->
               force
                 (case
                    (lessThanEqualsInteger x lim)
                    [ (delay (constr 0 []))
                    , (delay
                         (constr 1
                            [x, ((\x -> s s x) (addInteger 1 x) lim)])) ]))
            1
            1000))
      ((\s -> s s)
         (\s acc ds ->
            force
              (case
                 ds
                 [ (delay acc)
                 , (\x xs -> delay ((\x -> s s x) (addInteger acc x) xs)) ])))))