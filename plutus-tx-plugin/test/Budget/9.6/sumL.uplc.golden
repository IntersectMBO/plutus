(program
   1.1.0
   ((\ls ->
       (\s -> s s)
         (\s acc ds ->
            force
              (case
                 ds
                 [ (delay acc)
                 , (\x xs -> delay ((\x -> s s x) (addInteger acc x) xs)) ]))
         0
         ls)
      ((\s -> s s)
         (\s x lim ->
            force
              (force
                 (force ifThenElse
                    (lessThanEqualsInteger x lim)
                    (delay
                       (delay
                          (constr 1 [x, ((\x -> s s x) (addInteger 1 x) lim)])))
                    (delay (delay (constr 0 []))))))
         1
         1000)))