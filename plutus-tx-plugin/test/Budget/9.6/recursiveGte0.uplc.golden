(program
   1.1.0
   ((\go ->
       (\recursiveAll ->
          (\ls ->
             force recursiveAll
               (\v ->
                  force ifThenElse
                    (lessThanInteger v 0)
                    (constr 1 [])
                    (constr 0 []))
               ls)
            (go 1000))
         ((\s -> s s)
            (\s arg ->
               delay
                 (\ds ds ->
                    case
                      ds
                      [ (constr 0 [])
                      , (\x xs ->
                           case
                             (ds x)
                             [ (force (s s (delay (\x -> x))) ds xs)
                             , (constr 1 []) ]) ]))
            (delay (\x -> x))))
      ((\s -> s s)
         (\s n ->
            force
              (force ifThenElse
                 (lessThanEqualsInteger n 0)
                 (delay (constr 0 []))
                 (delay
                    (constr 1 [0, ((\x -> s s x) (subtractInteger n 1))])))))))