(program
   1.1.0
   ((fix rec.
       \x ds ->
         force
           (case
              ds
              [ (delay (constr 1 []))
              , (\x xs ->
                   delay
                     (force
                        (force
                           (force ifThenElse
                              (lessThanEqualsInteger 1 x)
                              (delay (delay (rec (addInteger 1 x) xs)))
                              (delay (delay (constr 0 [x]))))))) ]))
      0
      (constr 0 [])))