(program
   1.1.0
   ((\s -> s s)
      (\s ds ->
         force
           (case
              ds
              [ (delay (constr 1 []))
              , (\x xs ->
                   delay
                     (force
                        (case
                           (lessThanEqualsInteger 1 x)
                           [(delay (constr 0 [x])), (delay (s s xs))]))) ]))
      (constr 0 [])))