(program
   1.1.0
   ((\x0 ->
       (\s -> s s)
         (\s ds ->
            force
              (case ds [(delay 0), (\x xs -> delay (addInteger x (s s xs)))]))
         x0)
      ((\s -> s s)
         (\s x lim ->
            force
              (force
                 (force ifThenElse
                    (lessThanEqualsInteger x lim)
                    (delay
                       (delay
                          (constr 1 [x, ((\x -> s s x) (addInteger 1 x) lim)])))
                    (delay (delay (constr 0 []))))))
         1
         1000)))