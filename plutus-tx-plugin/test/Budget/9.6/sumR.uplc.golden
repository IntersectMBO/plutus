(program
   1.1.0
   ((\s -> s s)
      (\s ds ->
         force (case ds [(delay 0), (\x xs -> delay (addInteger x (s s xs)))]))
      ((\s -> s s)
         (\s x lim ->
            force
              (case
                 (lessThanEqualsInteger x lim)
                 [ (delay (constr 0 []))
                 , (delay
                      (constr 1 [x, ((\x -> s s x) (addInteger 1 x) lim)])) ]))
         1
         1000)))