let
  data Bool | Bool_match where
    True : Bool
    False : Bool
in
letrec
  data (List :: * -> *) a | List_match where
    Nil : List a
    Cons : a -> List a -> List a
in
letrec
  !go : integer -> List data -> data
    = \(ds : integer) (ds : List data) ->
        List_match
          {data}
          ds
          {all dead. data}
          (/\dead -> error {data})
          (\(x : data) (xs : List data) ->
             /\dead ->
               Bool_match
                 (ifThenElse {Bool} (equalsInteger 0 ds) True False)
                 {all dead. data}
                 (/\dead -> x)
                 (/\dead -> go (subtractInteger ds 1) xs)
                 {all dead. dead})
          {all dead. dead}
in
let
  data Unit | Unit_match where
    Unit : Unit
in
\(xs : List data) -> go 5 xs