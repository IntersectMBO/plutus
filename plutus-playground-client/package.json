{
  "name": "plutus-playground-client",
  "version": "1.0.0",
  "scripts": {
    "webpack": "DEBUG=purs-loader* DEBUG_DEPTH=100 webpack --progress --bail --mode=production -p",
    "webpack:server": "webpack-dev-server --progress --inline --hot --mode=development",
    "webpack:server:debug": "DEBUG=purs-loader* DEBUG_DEPTH=100 webpack-dev-server --progress --inline --hot",
    "purs:compile": "spago build",
    "purs:ide": "purs ide server --log-level=debug 'src/**/*.purs' 'generated/**/*.purs' 'test/**/*.purs' 'web-common/**/*.purs'",
    "test": "spago --no-psa test",
    "test:watch": "spago test --no-psa --watch",
    "docs": "spago docs",
    "repl": "spago repl",
    "start": "npm install && plutus-playground-generate-purs && npm run purs:compile && npm run webpack:server"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.2",
    "bignumber": "^1.1.0",
    "chartist": "^0.11.0",
    "chartist-plugin-axistitle": "^0.0.4",
    "chartist-plugin-tooltips": "^0.0.17",
    "extract-text-webpack-plugin": "^3.0.2",
    "jquery": "^3.3.1",
    "json-bigint": "^1.0.0",
    "monaco-editor": "^0.20.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "monaco-emacs": "^0.2.2",
    "monaco-vim": "^0.1.7",
    "popper.js": "^1.14.4",
    "uuid-validate": "^0.0.3",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "purs-loader": "^3.6.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2"
  },
  "resolutions": {},
  "license": "Apache-2.0"
}
