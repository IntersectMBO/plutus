(let
  (rec)
  (datatypebind
    (datatype
      (tyvardecl List (fun (type) (type)))
      (tyvardecl a (type))
      match_List
      (vardecl Nil [List a]) (vardecl Cons (fun a (fun [List a] [List a])))
    )
  )
  (let
    (rec)
    (termbind (strict)
      (vardecl
        foldl
        (all a (type) (all b (type) (fun (fun b (fun a b)) (fun b (fun [List a] b)))))
      )
      (abs a (type)
      (abs b (type)
      (lam
        f
        (fun b (fun a b))
        (lam
          acc
          b
          (lam
            lst
            [List a]
            [
              { [ { match_List a } lst ] b }
              acc
              (lam x a (lam xs [List a] [ { foldl a b } f [ f acc x ] xs ]))
            ]
          )
        )
      )
      )
      )
    )
    foldl
  )
)
