(let
  (nonrec)
  (termbind
    (strict)
    (vardecl addInteger (fun (con integer) (fun (con integer) (con integer))))
    (builtin addInteger)
  )
  (termbind
    (strict)
    (vardecl addInteger (fun (con integer) (fun (con integer) (con integer))))
    (lam x (con integer) (lam y (con integer) [ [ addInteger x ] y ]))
  )
  (termbind
    (nonstrict)
    (vardecl y (con integer))
    [ [ addInteger (con integer 1) ] (con integer 2) ]
  )
  (lam x (con integer) [ [ addInteger [ [ addInteger y ] x ] ] y ])
)
-- `y` should be float into the lambda, since the lambda is not on the RHS of a binding.
