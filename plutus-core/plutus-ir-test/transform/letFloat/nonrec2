(lam x (con integer)
(let (nonrec)
     (termbind (strict) (vardecl i2 (con integer)) (con integer 2))
     (let (nonrec)
          (termbind (strict) (vardecl i1 (con integer)) [(builtin addInteger) i2 (con integer 1)])
          (let (nonrec)
               (termbind (strict) (vardecl i (con integer))
               [(builtin addInteger) i1
                  [(builtin addInteger) x
                         [(builtin addInteger) i2
                          (let (nonrec)
                               (termbind (strict) (vardecl j (con integer)) (con integer 0)) j)]
               ]])
               [(builtin addInteger) i
                  (let (nonrec)
                  (termbind (strict) (vardecl j1 (con integer)) (con integer 0)) j1)
               ]
           )
      )
 ))
