(let
  (nonrec)
  (termbind (strict) (vardecl x (con integer)) 
   [ (lam z (con integer) z)
     (let
     (nonrec)
     (termbind
      (nonstrict)
      (vardecl y (con integer))
      (con integer 1)
     )
     y)
   ]
  )
  (con integer 1)
)
-- this test is to make sure that `y` can still float around but only inside/under `x`. see the similar strictNonValue
