dbConfig:
    dbConfigFile: scb-core.db
    dbConfigPoolSize: 20

scbWebserverConfig:
  baseUrl: http://localhost:8080
  staticDir: plutus-scb-client/dist

walletServerConfig:
  baseUrl: http://localhost:8081
  wallet:
    getWallet: 1

nodeServerConfig:
  mscBaseUrl: http://localhost:8082
  mscSocketPath: ./node-server.sock
  mscSlotLength: 5
  mscRandomTxInterval: 20
  mscBlockReaper:
    brcInterval: 600
    brcBlocksToKeep: 100
  mscInitialTxWallets:
    - getWallet: 1
    - getWallet: 2
    - getWallet: 3

chainIndexConfig:
  ciBaseUrl: http://localhost:8083

requestProcessingConfig:
  requestProcessingInterval: 1

signingProcessConfig:
  spBaseUrl: http://localhost:8084
  spWallet:
    getWallet: 1

metadataServerConfig:
  mdBaseUrl: http://localhost:8085

