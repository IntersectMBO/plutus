{
  "name": "marlowe-playground-client",
  "version": "1.0.0",
  "scripts": {
    "webpack": "webpack --progress --bail --mode=production --node-env=production",
    "webpack:server": "webpack-cli serve --progress --inline --hot --mode=development --node-env=development",
    "purs:compile": "spago build",
    "purs:ide": "purs ide server --log-level=debug 'src/**/*.purs' 'generated/**/*.purs' 'test/**/*.purs' 'web-common/**/*.purs' 'web-common-marlowe/**/*.purs'",
    "test": "webpack --config webpack.test.config.js --mode=development && node dist/test.js",
    "docs": "spago docs",
    "repl": "spago repl",
    "start": "npm install && marlowe-playground-generate-purs && npm run purs:compile && npm run webpack:server"
  },
  "resolutions": {},
  "license": "Apache-2.0",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.2",
    "bignumber": "^1.1.0",
    "blockly": "^4.20201217.0",
    "decimal.js": "^10.2.1",
    "json-bigint": "^1.0.0",
    "monaco-editor": "^0.21.0",
    "monaco-emacs": "^0.2.2",
    "monaco-vim": "^0.1.13",
    "moo": "^0.5.1",
    "nearley": "^2.20.0",
    "popper.js": "^1.14.4",
    "safe-eval": "^0.4.1",
    "vm-browserify": "^1.1.2",
    "xhr2": "^0.2.1"
  },
  "devDependencies": {
    "css-loader": "^5.2.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.5.1",
    "monaco-editor-webpack-plugin": "^2.1.0",
    "nearley-webpack-loader": "github:shmish111/nearley-webpack-loader#939360f9d1bafa9019b6ff8739495c6c9101c4a1",
    "node-sass": "^5.0.0",
    "purescript-psa": "^0.8.2",
    "purs-loader": "^3.7.2",
    "raw-loader": "^4.0.2",
    "sass-loader": "^11.0.1",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^3.0.0"
  }
}
